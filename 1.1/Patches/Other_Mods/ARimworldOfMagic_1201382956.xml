<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">	
	<mods>
		<li>A RimWorld of Magic</li>
	</mods>
	<match Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName = "Caravan_Oni_Speciality"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_SingleDef">
				<thingDef>RawMagicyte</thingDef>
				<price>Expensive</price>
				<countRange>
					<min>50</min>
					<max>150</max>
				</countRange>
			</li>
		</value>
	</match>
</Operation>

</Patch>