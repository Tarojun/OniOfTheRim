<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RulePackDef>
	<defName>NamerPersonOniMale</defName>
	<rulePack>
		<rulesStrings>
			<li>name->[malename] [lastname]</li>
		</rulesStrings>
		<rulesRaw>
			<li Class="Rule_File">
			  <keyword>lastname</keyword>
			  <path>OniNames/lastname</path>
			</li>
			<li Class="Rule_File">
				<keyword>malename</keyword>
				<path>OniNames/malename</path>
			</li>
		</rulesRaw>
	</rulePack>
</RulePackDef>

